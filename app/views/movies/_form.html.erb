<%= form_for @movie do |f| %>

<%= f.label :title %>
<%= f.text_field :title %>  

<%= f.label :director %>
<%= f.text_field :director %> <br>

<%= f.label :actors %>
<%= f.text_field :actors %> <br>

<%= f.label :release_year %>
<%= f.number_field :release_year %> <br>

<%= f.label :duration %>
<%= f.number_field :duration %> <br>

<%= f.submit %>

<% end -%>